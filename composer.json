{
    "name": "spress/spress",
    "description": "Static site generator with blogs support",
    "keywords": ["cms","blog","web","markdown","twig","silex","html"],
    "license": "MIT",
    "homepage": "http://spress.yosymfony.com",
    "authors": [
        {
            "name": "Victor Puertas",
            "email": "vpgugr@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "twig/twig": ">=1.8,<2.0-dev",
        "pimple/pimple": "~3.0",
        "symfony/yaml": "2.7.*",
        "symfony/console": "2.7.*",
        "symfony/finder": "2.7.*",
        "symfony/filesystem": "2.7.*",
        "symfony/event-dispatcher": "2.7.*",
        "michelf/php-markdown": "1.5.*",
        "dflydev/embedded-composer": "1.0.*@dev",
        "dflydev/apache-mime-types": "1.0.*",
        "yosymfony/config-loader": "1.3.*",
        "yosymfony/httpserver": "1.2.*",
        "yosymfony/resource-watcher": "1.1.*",
        "spress/spress-theme-spresso": "2.0.*",
        "erusev/parsedown-extra": "0.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
    "replace": {
        "spress/spress-core": "self.version"
    },
    "bin": [
        "bin/spress"
    ],
    "autoload": {
        "psr-4": {
            "Yosymfony\\Spress\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
